{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "rules": {
        "no-unused-vars": 1,
        "no-trailing-spaces": 1,
        "semi": 1,
        "quotes": 1,
        "prefer-const": 1,
        "comma-dangle" :1,
        "spaced-comment": 1,
        "eqeqeq": 0
    }
}